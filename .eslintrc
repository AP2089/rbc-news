{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "defineProps": "readonly",
    "defineEmits": "readonly",
    "withDefaults": "readonly"
  },
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:import/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "no-param-reassign": 2,
    "no-void": "off",
    "import/no-named-as-default": "off",
    "no-nested-ternary": 2,
    "max-classes-per-file": "off",
    "quotes": [2, "single", { "avoidEscape": true }],
    "quote-props": ["error", "consistent"],
    "no-unneeded-ternary": "off",
    "semi": [2, "always"],
    "comma-dangle": ["warn", "always-multiline"],
    "block-scoped-var": 2,
    "curly": 2,
    "default-case": 2,
    "default-case-last": 2,
    "dot-notation": 2,
    "eqeqeq": [2, "always"],
    "grouped-accessor-pairs": 2,
    "max-depth": [1, 4],
    "no-else-retur": [0, { "allowElseIf": false }],
    "no-empty-function": 2,
    "no-extra-boolean-cast": 1,
    "no-inline-comments": 1,
    "no-multi-assign": 2,
    "no-var": 2,
    "prefer-rest-param": 0,
    "require-await": 1,
    "sort-imports": [
      1,
      {
        "ignoreCase": false,
        "ignoreDeclarationSort": true,
        "ignoreMemberSort": true,
        "memberSyntaxSortOrder": ["none", "multiple", "all", "single"],
        "allowSeparatedGroups": false
      }
    ],

    "spaced-comment": [1, "always"],
    "array-bracket-spacing": [1, "never"],
    "arrow-parens": [1, "always"],
    "arrow-spacing": 1,
    "block-spacing": 1,
    "brace-style": [1, "1tbs", { "allowSingleLine": true }],
    "comma-spacing": [1, { "before": false, "after": true }],
    "computed-property-spacing": [1, "never"],
    "eol-last": [1, "always"],
    "func-call-spacing": [1, "never"],
    "function-call-argument-newline": [1, "consistent"],
    "key-spacing": [1, { "afterColon": true }],
    "keyword-spacing": [1, { "before": true, "after": true }],
    "linebreak-style": [1, "windows"],
    "lines-around-comment": [
      1,
      { "beforeLineComment": true, "allowBlockStart": true }
    ],
    "lines-between-class-members": [1, "always"],
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": 1,
    "no-trailing-spaces": 2,
    "no-whitespace-before-property": 2,
    "object-curly-spacing": [1, "always"],
    "padded-blocks": [2, { "blocks": "never" }],
    "padding-line-between-statements": [
      2,
      {
        "blankLine": "always",
        "prev": ["*"],
        "next": [
          "const",
          "let",
          "var",
          "block-like",
          "break",
          "case",
          "cjs-import",
          "class",
          "export",
          "function",
          "if",
          "import"
        ]
      },
      {
        "blankLine": "always",
        "prev": [
          "const",
          "let",
          "var",
          "block-like",
          "break",
          "case",
          "cjs-import",
          "class",
          "export",
          "function",
          "if",
          "import"
        ],
        "next": "*"
      },
      {
        "blankLine": "any",
        "prev": ["const", "let", "var", "cjs-import", "import"],
        "next": ["const", "let", "var", "cjs-import", "import"]
      }
    ],
    "semi-spacing": [1, { "before": false, "after": true }],
    "semi-style": [2, "last"],
    "space-before-blocks": 2,
    "space-before-function-paren": "off",
    "space-in-parens": [2, "never"],
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error"],
    "no-useless-computed-key": "off",
    "import/extensions": ["error", "always"],
    "import/no-unresolved": [2, { "ignore": ["\\.json$"] }],
    "camelcase": "off",

    // vue
    "vue/html-self-closing": [
      1,
      {
        "html": {
          "void": "always",
          "normal": "always",
          "component": "always"
        }
      }
    ],
    "vue/no-v-html": "off",
    "vue/attribute-hyphenation": "off",
    "vue/no-multiple-template-root": "off",
    "vue/multi-word-component-names": "off",
    "vue/no-v-model-argument": "off",
    "vue/html-closing-bracket-spacing": [
      1,
      {
        "startTag": "never",
        "endTag": "never",
        "selfClosingTag": "always"
      }
    ],
    "vue/html-indent": [
      2,
      2,
      {
        "attribute": 1,
        "baseIndent": 1,
        "closeBracket": 0,
        "alignAttributesVertically": true,
        "ignores": []
      }
    ],
    "vue/html-quotes": [2, "double", { "avoidEscape": false }],
    "vue/max-attributes-per-line": [
      1,
      {
        "singleline": {
          "max": 10
        },
        "multiline": {
          "max": 1
        }
      }
    ],
    "vue/singleline-html-element-content-newline": "off",
    "vue/multiline-html-element-content-newline": [
      2,
      {
        "ignoreWhenEmpty": true,
        "allowEmptyLines": true
      }
    ],
    "vue/mustache-interpolation-spacing": [1, "always"],
    "vue/no-multi-spaces": 1,
    "vue/no-spaces-around-equal-signs-in-attribute": [2],
    "vue/no-template-shadow": 2,
    "vue/prop-name-casing": [2, "camelCase"],
    "vue/require-default-prop": 2,
    "vue/require-prop-types": 2,
    "vue/v-slot-style": [
      1,
      {
        "atComponent": "shorthand",
        "default": "shorthand",
        "named": "shorthand"
      }
    ],
    "vue/require-explicit-emits": 2,
    "vue/v-on-event-hyphenation": [
      1,
      "never",
      {
        "autofix": true,
        "ignore": []
      }
    ],
    "vue/attributes-order": [
      1,
      {
        "order": [
          "DEFINITION",
          "LIST_RENDERING",
          "CONDITIONALS",
          "RENDER_MODIFIERS",
          "GLOBAL",
          ["UNIQUE", "SLOT"],
          "TWO_WAY_BINDING",
          "OTHER_DIRECTIVES",
          "OTHER_ATTR",
          "EVENTS",
          "CONTENT"
        ],
        "alphabetical": false
      }
    ],
    "vue/component-tags-order": [
      2,
      {
        "order": ["script", "template", "style"]
      }
    ],
    "vue/no-lone-template": [
      2,
      {
        "ignoreAccessible": false
      }
    ],
    "vue/order-in-components": [
      2,
      {
        "order": [
          "name",
          ["props", "propsData"],
          "emits",
          "components",
          "mixins",
          "data",
          "computed",
          "watch",
          "directives",
          "methods",
          "LIFECYCLE_HOOKS",
          "ROUTER_GUARDS",
          "template",
          "render",
          "renderError",
          "el",
          "key",
          "parent",
          "functional",
          ["delimiters", "comments"],
          "fetch",
          "asyncData",
          "watchQuery",
          "filters",
          "extends",
          ["provide", "inject"],
          "layout",
          "middleware",
          "validate",
          "scrollToTop",
          "transition",
          "loading",
          "inheritAttrs",
          "model",
          "setup",
          "head"
        ]
      }
    ],
    "vue/this-in-template": [2, "never"],
    "vue/block-lang": [
      2,
      {
        "script": {
          "lang": "ts"
        },
        "style": {
          "lang": ["scss", "css"]
        }
      }
    ],
    "vue/block-tag-newline": [
      2,
      {
        "singleline": "always",
        "multiline": "always",
        "maxEmptyLines": 0
      }
    ],
    "vue/component-name-in-template-casing": [
      "error",
      "PascalCase",
      {
        "registeredComponentsOnly": false
      }
    ],
    "vue/custom-event-name-casing": [
      1,
      "camelCase",
      {
        "ignores": ["/^[a-z]+(?:-[a-z]+)*:[a-z]+(?:-[a-z]+)*$/u"]
      }
    ],
    "vue/html-comment-content-newline": [
      1,
      {
        "singleline": "never",
        "multiline": "always"
      },
      {
        "exceptions": []
      }
    ],
    "vue/html-comment-content-spacing": [
      1,
      "always",
      {
        "exceptions": []
      }
    ],
    "vue/html-comment-indent": [1, 2],
    "vue/match-component-file-name": [
      2,
      {
        "extensions": ["vue", "js", "ts"],
        "shouldMatchCase": true
      }
    ],
    "vue/new-line-between-multi-line-property": [
      1,
      {
        "minLineOfMultilineProperty": 2
      }
    ],
    "vue/no-boolean-default": [2, "default-false"],
    "vue/no-empty-component-block": 2,
    "vue/no-multiple-objects-in-class": 1,
    "vue/no-potential-component-option-typo": [
      2,
      {
        "presets": ["all"]
      }
    ],
    "vue/no-reserved-component-names": [
      2,
      {
        "disallowVue3BuiltInComponents": true
      }
    ],
    "vue/no-template-target-blank": [
      1,
      {
        "allowReferrer": true,
        "enforceDynamicLinks": "always"
      }
    ],
    "vue/no-this-in-before-route-enter": 2,
    "vue/no-unused-properties": [
      "error",
      {
        "groups": ["props", "computed"]
      }
    ],
    "vue/no-unused-refs": 2,
    "vue/no-use-computed-property-like-method": 2,
    "vue/padding-line-between-blocks": [2, "always"],
    "vue/require-direct-export": 2,
    "vue/v-for-delimiter-style": [2, "in"],
    "vue/v-on-function-call": [2, "never"],
    "@typescript-eslint/explicit-function-return-type": "off",
    "no-debugger": "off",
    "no-console": "off"
  }
}
